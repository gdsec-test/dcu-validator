[flake8]
# E501 line too long
ignore = E501
max-line-length = 120
exclude =
    # Don't check proto generated files as these shouldn't be edited
    scheduler_service/grpc_stub/schedule_service_pb2.py,
    scheduler_service/grpc_stub/schedule_service_pb2_grpc.py
    # imports that appear to be unused in route.py are necessary in order to provide classes that define handlers
    scheduler_service/validators/route.py,
    .venv
